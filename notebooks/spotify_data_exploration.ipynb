    "\n",
    "#make the emotions collide with what we have\n",
    "go_map = {\n",
    "    \"happy\":[\"pride\",\"admiration\",\"amusement\",\"excitement\",\"love\",\"optimism\",\"gratitude\",\"relief\",\"joy\"],\n",
    "    \"sad\":[\"disappointment\",\"remorse\",\"sadness\",\"grief\"],\n",
