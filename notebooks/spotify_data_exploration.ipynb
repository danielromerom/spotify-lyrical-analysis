    "other_cols = [c for c in X_train.columns if not c.startswith('bert_')]\n",
    "all_numeric_cols = numeric_cols + bert_cols\n",
    "scaler = RobustScaler()\n",
    "X_train_scaled = X_train.copy()\n",
    "X_test_scaled = X_test.copy()\n",
    "\n",
